Host me.github.com
    HostName github.com
    PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/me_rsa

Host work.github.com
    HostName github.com
    PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/work_rsa

Host a.ghub.com
    HostName github.com
    PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/a.ghub.key
